<!DOCTYPE html>
<html>
<head>
  <title>Music</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <p>
    <%= link_to "Genres", genres_path, class: 'btn btn-info btn-s' %>
    <%= link_to "Artists", artists_path, class: 'btn btn-info btn-s' %>
    <%= link_to "Songs", songs_path, class: 'btn btn-info btn-s' %>
  </p>

  <%= yield %>
</div>
</body>
</html>
